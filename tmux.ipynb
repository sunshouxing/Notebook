   "source": [
    "### session\n",
    "\n",
    "* create new session\n",
    "```\n",
    "tmux new -s <name-of-my-session>\n",
    "or \n",
    "ctrl-a : new -s <name-of-my-session>\n",
    "```\n",
    "\n",
    "* list exist session\n",
    "```\n",
    "tmux list-clients\n",
    "```\n",
    "\n",
    "* attach exist session\n",
    "```\n",
    "tmux attach -t <exist-session-name>\n",
    "or\n",
    "ctrl-a s <session-number>\n",
    "```\n",
    "\n",
    "* rename session\n",
    "```\n",
    "ctrl-a $ \n",
    "```"
   ]
  }
